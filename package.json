{
  "name": "surveygizmo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www | npm run watch",
    "build": "browserify -t bubleify -t [ envify --NODE_ENV production ] ./src/app.js  > ./public/javascripts/bundle.js",
    "build-dev": "browserify -d -t bubleify ./src/app.js > ./public/javascripts/bundle.js",
    "watch": "watchify -d -t bubleify ./src/app.js -o ./public/javascripts/bundle.js -v"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "body-parser": "~1.16.0",
    "connect-redis": "^3.2.0",
    "cookie-parser": "~1.4.3",
    "datasets-us-states-names-abbr": "^1.0.0",
    "debug": "~2.6.9",
    "edx-oauth-middleware": "0.0.5",
    "express": "~4.14.1",
    "express-compile-sass": "^3.0.4",
    "express-session": "^1.15.1",
    "flux": "^3.1.2",
    "hasha": "^2.2.0",
    "jade": "~1.11.0",
    "mongodb": "^2.2.24",
    "mongoose": "^4.9.0",
    "morgan": "~1.7.0",
    "node-sass": "^4.5.3",
    "nodemailer": "^3.1.8",
    "nodemon": "^1.11.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.3",
    "react-paginate": "^4.2.0",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "browserify": "^14.4.0",
    "bubleify": "^0.7.0",
    "envify": "^4.1.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "watchify": "^3.9.0"
  }
}
